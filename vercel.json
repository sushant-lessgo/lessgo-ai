{
  "crons": [
    {
      "path": "/api/cron/sync-analytics",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/forms/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, max-age=0, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
